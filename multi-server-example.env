# Example: Multiple Rundeck Servers Configuration
# This shows how to configure multiple Rundeck environments

# === Production Environment ===
RUNDECK_URL=https://rundeck-prod.company.com
RUNDECK_API_TOKEN=prod-api-token-12345
RUNDECK_API_VERSION=47

# === Additional Environments ===

# Staging Environment
RUNDECK_URL_1=https://rundeck-staging.company.com
RUNDECK_API_TOKEN_1=staging-api-token-67890
RUNDECK_API_VERSION_1=47
RUNDECK_NAME_1=staging

# Development Environment
RUNDECK_URL_2=https://rundeck-dev.company.com
RUNDECK_API_TOKEN_2=dev-api-token-abcde
RUNDECK_API_VERSION_2=47
RUNDECK_NAME_2=development

# Testing Environment
RUNDECK_URL_3=https://rundeck-test.company.com
RUNDECK_API_TOKEN_3=test-api-token-fghij
RUNDECK_API_VERSION_3=47
RUNDECK_NAME_3=testing

# Usage Examples:
# - list_servers                           # Shows all configured servers
# - get_projects(server="staging")         # Get projects from staging
# - get_jobs(project="web", server="dev")  # Get jobs from development
# - run_job(job_id="123", server="prod")   # Run job on production